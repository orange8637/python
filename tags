BUFSIZE	tcpS.py	/^BUFSIZE = 1024$/;"	v
BUFSIZE	udpC.py	/^BUFSIZE = 1024$/;"	v
BUFSIZE	udpS.py	/^BUFSIZE = 1024$/;"	v
HOST	tcpC.py	/^HOST = '192.168.1.103'$/;"	v
HOST	tcpS.py	/^HOST = ''$/;"	v
HOST	udpC.py	/^HOST = '192.168.1.105'$/;"	v
HOST	udpS.py	/^HOST = ''$/;"	v

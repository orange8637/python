PORT	udpS.py	/^PORT = 2222$/;"	v
binary_search	demo.c	/^int binary_search(int *data, int low, int high, int value)$/;"	f
ctime	tcpS.py	/^from time import ctime$/;"	i
ctime	udpS.py	/^from time import ctime$/;"	i
data	tcpC.py	/^	data = raw_input('>>> ')$/;"	v
data	tcpC.py	/^	data = tcpCliSock.recv(BUFSIZE)$/;"	v
data	tcpS.py	/^		data = tcpCliSock.recv(BUFSIZE)$/;"	v
data	udpC.py	/^	data = raw_input('>> ')$/;"	v
fib	demo.py	/^def fib(n):$/;"	f
main	c_time.cpp	/^int main()$/;"	f
main	demo.c	/^int main()$/;"	f
sys	demo.py	/^import sys$/;"	i
